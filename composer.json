{
    "name": "neoxygen/neogen",
    "type": "library",
    "description": "Neo4j Graph generator for PHP5.4+",
    "keywords": [
        "graph",
        "neo4j",
        "generator"
    ],
    "homepage": "http://neoxygen.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Willemsen",
            "email": "chris@neoxygen.io"
        }
    ],
    "require": {
        "php": ">=5.4",
        "fzaninotto/faker": "~1.4",
        "symfony/console": "~2.5",
        "symfony/yaml": "~2.5",
        "symfony/finder": "~2.5",
        "symfony/dependency-injection": "~2.5",
        "neoxygen/neoclient": "~2.0",
        "ikwattro/faker-extra": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpspec/phpspec": "2.1.0-RC1"
    },
    "autoload": {
        "psr-4": {
            "Neoxygen\\Neogen\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neoxygen\\Neogen\\Tests\\": "tests/Neoxygen/Neogen/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": ["bin/neogen"]
}
